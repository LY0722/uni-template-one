<template name="b954f366">
  <view class="_view data-v-39305030 page">
    <page-head title="{{title}}" class="_page-head data-v-39305030"></page-head>
    <view class="_view data-v-39305030 product-list">
      <view class="_view data-v-39305030 product" wx:key="index" key="{{index}}" wx:for="{{shopList}}" wx:for-index="index" wx:for-item="product">
        <view class="_view data-v-39305030 image-view">
          <image wx:if="{{renderImage}}" class="_image data-v-39305030 product-image" src="{{product.image}}"></image>
        </view>
        <view class="_view data-v-39305030 product-title">{{product.title}}</view>
        <view class="_view data-v-39305030 product-price">
          <text class="_text data-v-39305030 product-price-favour">￥{{product.originalPrice}}</text>
          <text class="_text data-v-39305030 product-price-original">￥{{product.favourPrice}}</text>
          <text class="_text data-v-39305030 product-tip">{{product.tip}}</text>
        </view>
      </view>
    </view>
  </view>
</template>