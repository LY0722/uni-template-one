
.my_wrap {
  background: #EFEFEF;
  min-height: 100vh;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.logo {
  height: 240rpx;
  padding: 20rpx;
  box-sizing: border-box;
  background-color: #FF80AB;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.logo-hover {
  opacity: 0.8;
}
.logo-img {
  width: 150rpx;
  height: 150rpx;
  border-radius: 150px;
}
.logo-title {
  height: 150rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  margin-left: 20rpx;
}
.uer-name {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 300rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 38rpx;
  color: #FFFFFF;
}
.login-title {
  height: 150rpx;
  -webkit-box-align: self-start;
  -webkit-align-items: self-start;
  align-items: self-start;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  margin-left: 20rpx;
}
.login-icon-color {
  color: #fff;
}
.center-list {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  margin-top: 20rpx;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
}
.center-list-item {
  height: 90rpx;
  box-sizing: border-box;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  padding: 0rpx 20rpx;
  background-color: #FFFFFF;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.border-bottom {
  border-bottom-width: 1rpx;
  border-color: #c8c7cc;
  border-bottom-style: solid;
}
.list-icon {
  width: 40rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 34rpx;
  color: #FF80AB;
  text-align: center;
  font-family: texticons;
  margin-right: 20rpx;
}
.list-text {
  padding-left: 15rpx;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 34rpx;
  color: #555;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  text-align: left;
}
.my-order {
  background: #fff;
  padding: 0 20rpx;
  display: block;
}
.my-order-title {
  font-size: 34rpx;
  color: #555;
  height: 90rpx;
  line-height: 90rpx;
}
.my-order-list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.my-order-item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  display: block;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  text-align: center;
}
.my-order-icon {
  display: block;
  text-align: center;
  font-size: 55rpx;
}
.my-order-title {
  display: block;
}